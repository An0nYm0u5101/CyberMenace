<setup>
	<block title="CyberMenace">
		<text>Coniguration Page for CyberMenace</text>
	</block>

	<block title="Input Controls"
		endpoint="CyberMenace/configurator" entity="setupentries">

		<input field="address">
			<label>Enter Firewall or Reciever IP Address or hostname</label>
			<type>receiver</type>
		</input>

		<input field="port">
			<label>Enter Port for Firewall or Reciever</label>
			<type>rport</type>
		</input>
	</block>
</setup>
